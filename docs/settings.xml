<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 http://maven.apache.org/xsd/settings-1.0.0.xsd">
	<servers>
		<server>
			<id>predix.repo</id>
			<username>(predix.io.username.here)</username>
			<password>(predix.io.encrypted.password.here use mvn --encrypt-password</password>
		</server>
	</servers>
	<!--<proxies>
                <proxy>
                        <id>optional</id>
                        <active>true</active>
                        <protocol>http</protocol>
                        <host>your-proxy-server-here</host>
                        <port>your-proxy-port-here</port>
                        <nonProxyHosts>*.yourcompany.com here</nonProxyHosts>
                </proxy>
        </proxies>-->
	<profiles>
		<profile>
	                <id>default</id>
			<repositories>
				<repository>
					<id>central</id>
					<url>http://repo1.maven.org/maven2</url>
					<snapshots>
						<enabled>true</enabled>
					</snapshots>
					<releases>
						<enabled>true</enabled>
					</releases>
				</repository>
				<repository>
					<id>predix.repo</id>
					<url>https://artifactory.predix.io/artifactory/PREDIX-EXT</url>
					<snapshots>
						<enabled>false</enabled>
					</snapshots>
					<releases>
						<enabled>true</enabled>
					</releases>
				</repository>
			</repositories>
		</profile>
	</profiles>
	<activeProfiles>
    		<activeProfile>default</activeProfile>
  	</activeProfiles>
</settings>
